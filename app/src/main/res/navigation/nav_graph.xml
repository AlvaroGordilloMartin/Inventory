<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/dashBoardFragment">
    <fragment
        android:id="@+id/dashBoardFragment"
        android:name="com.example.inventorynavigation.iu.DashBoardFragment"
        android:label="fragment_dash_board"
        tools:layout="@layout/fragment_dash_board" >
        <action
            android:id="@+id/action_dashBoardFragment_to_listDependencyFragment"
            app:destination="@id/listDependencyFragment" />
        <action
            android:id="@+id/action_dashBoardFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/listDependencyFragment"
        android:name="com.example.inventorynavigation.iu.dependency.ListDependencyFragment"
        android:label="fragment_list_dependency"
        tools:layout="@layout/fragment_list_dependency" >
        <action
            android:id="@+id/action_listDependencyFragment_to_addEditListDependencyFragment"
            app:destination="@id/addEditListDependencyFragment" />
        <action
            android:id="@+id/action_listDependencyFragment_to_baseDialogFragment"
            app:destination="@id/baseDialogFragment" />
    </fragment>
    <fragment
        android:id="@+id/addEditListDependencyFragment"
        android:name="com.example.inventorynavigation.iu.addedit.AddEditListDependencyFragment"
        android:label="fragment_add_edit_list_dependency"
        tools:layout="@layout/fragment_add_edit_list_dependency" >
        <argument
            android:name="dependency"
            app:argType="com.example.inventorynavigation.data.model.Dependency" />
        <action
            android:id="@+id/action_addEditListDependencyFragment_to_listDependencyFragment"
            app:destination="@id/listDependencyFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.inventorynavigation.iu.preferences.SettingsFragment"
        android:label="SettingsFragment" >
        <action
            android:id="@+id/action_settingsFragment_to_accountFragment"
            app:destination="@id/accountFragment" />
    </fragment>
    <fragment
        android:id="@+id/accountFragment"
        android:name="com.example.inventorynavigation.iu.preferences.AccountFragment"
        android:label="AccountFragment" />
    <dialog
        android:id="@+id/baseDialogFragment"
        android:name="com.example.inventorynavigation.iu.base.BaseDialogFragment"
        android:label="BaseDialogFragment" />
</navigation>